{
  "defines": {
    ".node-version": { "format": "text" }
  },
  "patches": {
    ".node-version": [
      {
        "value": "20",
        "priority": -1
      }
    ],
    ".gitignore": [
      {
        "value": ["/node_modules"]
      }
    ],
    ".prettierignore": [
      {
        "value": ["/package-lock.json"]
      }
    ],
    ".vscode/extensions.json": [
      {
        "value": { "recommendations": ["eg2.vscode-npm-script"] }
      }
    ],
    ".vscode/settings.json": [
      {
        "value": { "files.exclude": { ".node-version": true } }
      }
    ]
  }
}
