{
  "patches": {
    ".vscode/extensions.json": [
      {
        "value": { "recommendations": ["dbaeumer.vscode-eslint"] }
      }
    ],
    ".vscode/settings.json": [
      {
        "value": {
          "editor.codeActionsOnSave": { "source.fixAll.eslint": "explicit" },
          "eslint.experimental.useFlatConfig": true
        }
      }
    ]
  }
}
