{
  "patches": {
    ".github/workflows/ci.yml": [
      {
        "value": {
          "jobs": {
            "CI": {
              "steps": [
                {"uses": "denoland/setup-deno@v1", "with": {"deno-version": "v1.x"}},
                {"run": "deno fmt --check"},
                {"run": "deno lint"},
                {"run": "deno test --allow-read"},
                {"run": "deno bench --allow-read"}
              ]
            }
          }
        }
      }
    ]
  }
}
