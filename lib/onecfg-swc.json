{
  "defines": {
    ".swcrc": { "format": "json" }
  },
  "patches": {
    ".swcrc": [
      {
        "value": {
          "jsc": { "target": "es2022" },
          "module": { "type": "es6" },
          "sourceMaps": true
        },
        "priority": -1
      }
    ],
    ".prettierignore": [
      {
        "value": ["/.swcrc"]
      }
    ],
    ".vscode/settings.json": [
      {
        "value": { "files.exclude": { ".swcrc": true } }
      }
    ]
  }
}
